---
import LayoutCommon from '../app/LayoutCommon.astro';
import PublicNavbar from '../commonComponents/PublicNavbar.astro';
import { envConfig } from '../config/envConfig';
const cookie = Astro.cookies.get("platform-data")?.value;
const env = import.meta.env || process.env;

---

<LayoutCommon>
	<div class="flex flex-col min-h-screen bg-white dark:bg-gray-900 -z-10">
		<header>
			<PublicNavbar limitedWidth={true} />
		</header>

		<main class="grow" transition:animate="slide">
			<section
				class="bg-white dark:bg-gray-900 bg-no-repeat bg-cover bg-center bg-[url('/images/blue_line_bg.svg')] pb-32 md:pb-44"
			>
				<div class="grid grid-cols-1 md:grid-cols-2 max-w-screen-xl mx-auto">
					<div class="col-span-2">
						<div class="py-4 px-4 lg:py-16 lg:px-12 flex items-center flex-col">
							<div class="flex justify-center">
								<a
									href="https://github.com/credebl/platform/releases"
									class="inline-flex justify-between items-center py-1 px-1 pr-4 mb-7 text-sm text-gray-700 bg-gray-100 rounded-full dark:bg-gray-800 dark:text-white hover:bg-gray-200 dark:hover:bg-gray-700"
									role="alert"
								>
									<span
										class="text-xs bg-primary-600 rounded-full text-white px-4 py-1.5 mr-3"
										>New</span
									>
									<span class="text-sm font-medium"
										>CREDEBL 2.0 is out! See what's new</span
									>
									<svg
										class="ml-2 w-5 h-5"
										fill="currentColor"
										viewBox="0 0 20 20"
										xmlns="http://www.w3.org/2000/svg"
										><path
											fill-rule="evenodd"
											d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
											clip-rule="evenodd"></path></svg
									>
								</a>
							</div>

							<h1
								class="md:mb-4 md:text-center md:flex md:justify-center font-extrabold tracking-tight leading-none text-gray-900 lg:text-4xl md:text-xl sm:text-sm dark:text-white lg:max-w-[736px] md:max-w-[430px] sm:max-w-[365px]"
							>
								Build Self-Sovereign Identity (SSI) solutions faster - for
								global scale.
							</h1>
							<p
								class="mt-8 mb-8 md:flex md:text-center md:justify-center font-normal text-gray-500 lg:text-xl md:text-sm sm:text-xs dark:text-gray-400 lg:max-w-[583px] md:max-w-[409px] sm:max-w-[333px]"
							>
								CREDEBL aims to significantly reduce the tedious work required
								to build SSI solutions from scratch.
							</p>
							<div class="justify-center flex">
								<a
									href="https://docs.credebl.id/en/intro/what-is-credebl/"
									class="inline-flex justify-center items-center py-3 px-5 text-base font-medium text-center text-white rounded-lg bg-primary-700 hover:bg-primary-800 focus:ring-4 focus:ring-primary-300 dark:focus:ring-primary-900"
								>
									Get Started
									<svg
										class="ml-2 -mr-1 w-5 h-5"
										fill="currentColor"
										viewBox="0 0 20 20"
										xmlns="http://www.w3.org/2000/svg"
										><path
											fill-rule="evenodd"
											d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z"
											clip-rule="evenodd"></path></svg
									>
								</a>
							</div>
							<div class="mt-8 flex flex-col items-center">
								<div class="flex items-center">
									<div class="flex mr-2">
										<img
											src="/images/CREDEBL_ICON.png"
											class="h-8 mr-3"
											alt="CREDEBL Logo"
										/>
										<span
											class="self-center text-xl font-semibold sm:text-2xl whitespace-nowrap dark:text-white"
											>CREDEBL</span
										>
									</div>
									<span
										class="font-normal text-gray-500 lg:text-xl md:text-sm sm:text-xs dark:text-gray-400"
										>has been recognized by</span
									>
								</div>
								<a href="https://app.digitalpublicgoods.net/a/10902" target="_blank" class="block dark:hidden hover:bg-gray-50 p-4 rounded-md">
									<img
									class="mt-6"
									src="/images/DPG-alliance.png"
									width="173px"
									/>
								</a>
								<a href="https://app.digitalpublicgoods.net/a/10902" target="_blank" class="hidden dark:block hover:bg-gray-800 p-4 rounded-md">
									<img
									class="mt-6"
									src="/images/dark-dpg-alliance.svg"
									width="173px"
									/>
								</a>
							</div>
						</div>
					</div>
				</div>
			</section>
			<section class="md:p-8 sm:p-4 p-4">
				<div
					class="col-span-2 flex justify-center mx-auto -mt-36 md:pl-12 md:pr-12 sm:pl-3 sm:pr-3 max-w-screen-xl"
				>
					<img
						src="/images/org-dashboard-light.png"
						alt=""
						class="max-w-screen-xl w-full block dark:hidden rounded-lg shadow-[0_0_12px_12px_rgba(0,0,0,0.1)]"
					/>
					<img
						src="/images/org-dashboard-dark.png"
						alt=""
						class="max-w-screen-xl w-full dark:block hidden rounded-lg shadow-[0_0_12px_12px_rgba(0,0,0,0.1)]"
					/>
				</div>
			</section>
			<section
				class="bg-gray-100 dark:bg-gray-800 mx-auto p-4 py-10 md:py-12 md:pl-12 md:pr-12 sm:pl-3 sm:pr-3"
			>
				<div class="max-w-screen-xl mx-auto p-4 lg:pl-12">
					<div class="pb-8 lg:pb-4 mx-auto">
						<h2
							class="mb-4 text-2xl lg:text-3xl tracking-tight leading-tight text-center font-extrabold text-gray-900 dark:text-white"
						>
							Build trusted digital ecosystems using the SSI core.
						</h2>
						<p class="text-gray-500 sm:text-xl dark:text-gray-400 text-center">
							CREDEBL helps you build trusted digital ecosystems, by leveraging
							global standards of <a
								href="https://www.w3.org/TR/did-core/"
								target="_blank">W3C Decentralized Identifiers (DID)</a
							> & <a href="https://www.w3.org/TR/vc-data-model/" target="_blank"
								>Verifiable Credentials (VC)</a
							> standards.
						</p>
					</div>
					<div
						class="grid md:grid-cols-2 grid-cos-1 md:px-28 md:gap-x-16 lg:gap-x-28 sm:gap-x-12 gap-y-8 md:gap-y-16"
					>
						<div class="mt-8 flex">
							<div
								class="flex mr-8 justify-center items-center mb-4 w-10 h-10 rounded-full bg-primary-100 lg:h-12 lg:w-12 dark:bg-primary-900 shrink-0"
							>
								<svg
									xmlns="http://www.w3.org/2000/svg"
									width="20"
									height="19"
									viewBox="0 0 20 19"
									fill="none"
								>
									<path
										fill-rule="evenodd"
										clip-rule="evenodd"
